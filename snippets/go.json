{
  "test": {
    "prefix": "test",
    "body": "func Test${1:FunctionName}(t *testing.T) {\n\t$0\n}",
    "description": "Go testing pattern"
  },
  "errcheck": {
    "prefix": "err",
    "body": "if err != nil {\n\tlog.Fatalf(\"Error${1:}: %v\", err)\n}",
    "description": "Go error handling pattern"
  }
}
